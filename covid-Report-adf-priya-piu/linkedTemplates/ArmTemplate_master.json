{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-Report-adf-priya-piu"
		},
		"ls_blob_covidreport1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_covidreport1'"
		},
		"ls_datalake_covidreport2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake_covidreport2'"
		},
		"ls_datalake_covidreport2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreport2datalake.dfs.core.windows.net/"
		},
		"ls_opendata_ecdc_europa_eu_priya_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-Report-adf-priya-piu_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_covidreport1_connectionString": {
						"value": "[parameters('ls_blob_covidreport1_connectionString')]"
					},
					"ls_datalake_covidreport2_accountKey": {
						"value": "[parameters('ls_datalake_covidreport2_accountKey')]"
					},
					"ls_datalake_covidreport2_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_covidreport2_properties_typeProperties_url')]"
					},
					"ls_opendata_ecdc_europa_eu_priya_properties_typeProperties_url": {
						"value": "[parameters('ls_opendata_ecdc_europa_eu_priya_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-Report-adf-priya-piu_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-Report-adf-priya-piu_ArmTemplate_0')]"
			]
		}
	]
}